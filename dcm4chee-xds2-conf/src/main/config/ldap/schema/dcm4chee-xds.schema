objectclass ( 1.2.40.0.13.1.15.20.4.1 
	NAME 'xdsRegistry' 
	DESC 'XDS Application information' 
	SUP top 
	STRUCTURAL 
	MUST ( xdsApplicationName $ xdsAffinityDomain $ xdsAffinityDomainConfigDir $ cn ) 
	MAY ( xdsAcceptedMimeTypes $ xdsSoapMsgLogDir $ xdsCreateMissingPIDs $ xdsCreateMissingCodes $ xdsDontSaveCodeClassifications $ xdsCheckAffinityDomain $ xdsCheckMimetype $ xdsPreMetadataCheck ) 
 )

objectclass ( 1.2.40.0.13.1.15.20.4.2 
	NAME 'xdsRepository' 
	DESC 'XDS Application information' 
	SUP top 
	STRUCTURAL 
	MUST ( xdsApplicationName $ xdsRegistryURL $ xdsRepositoryUID $ cn ) 
	MAY ( xdsAcceptedMimeTypes $ xdsCheckMimetype $ xdsSoapMsgLogDir $ xdsAllowedCipherHostname $ xdsForceMTOM $ xdsLogFullMessageHosts ) 
 )

objectclass ( 1.2.40.0.13.1.15.20.4.3 
	NAME 'xcaRespondingGW' 
	DESC 'XCA Responding Gateway information' 
	SUP top 
	STRUCTURAL 
	MUST ( xdsApplicationName $ xdsHomeCommunityID $ xdsRegistryURL $ xdsRepositoryURL $ cn ) 
	MAY xdsSoapMsgLogDir 
 )

objectclass ( 1.2.40.0.13.1.15.20.4.4 
	NAME 'xcaInitiatingGW' 
	DESC 'XCA Initiating Gateway information' 
	SUP top 
	STRUCTURAL 
	MUST ( xdsApplicationName $ xdsHomeCommunityID $ xdsRespondingGatewayURL $ cn ) 
	MAY ( xdsRespondingGatewayRetrieveURL $ xdsRegistryURL $ xdsRepositoryURL $ xdsAsync $ xdsAsyncHandler $ xdsPIXConsumerApplication $ xdsPIXManagerApplication $ xdsAssigningAuthority $ xdsSoapMsgLogDir ) 
 )

objectclass ( 1.2.40.0.13.1.15.20.4.5 
