# dcm4chee-xds extensions of the DICOM Application Configuration Data Model Hierarchy LDAP Schema
dn: cn=schema
objectClass: top
objectClass: ldapSubentry
objectClass: subschema
attributeTypes: ( 1.2.40.0.13.1.15.20.3.1
  NAME 'xdsApplicationName'
  DESC 'XDS Application name'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.2 NAME 'xdsAffinityDomain'
  DESC 'Affinity Domain given as Universal Entity ID and Universal Entity ID Type ISO (e.g.: 1.2.3.4.5&ISO)'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.3 NAME 'xdsAcceptedMimeTypes'
  DESC 'Accepted MIME types in the affinity domain'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.4 NAME 'xdsSoapMsgLogDir'
  DESC 'Log directory where request and response SOAP messages are stored'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.5 NAME 'xdsCreateMissingPIDs'
  DESC 'Specifies to create Patient IDs that are not yet known. (not conform to XDS specification!)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.6 NAME 'xdsCreateMissingCodes'
  DESC 'Specifies to create Codes that are not known in the Affinity Domain. (not conform to XDS specification!)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.7 NAME 'xdsDontSaveCodeClassifications'
  DESC 'Specifies to save codes only as XDSCode entities and not as Classifications in ebRIM format'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.8 NAME 'xdsCheckAffinityDomain'
  DESC 'Check affinityDomain in received PatientIDs (Patient feed and preMetadataCheck)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.9 NAME 'xdsCheckMimetype'
  DESC 'Enable check  of mimetype'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.10 NAME 'xdsPreMetadataCheck'
  DESC 'Check metadta before processing the PnR request. (to get correct error (XDSPatientIdDoesNotMatch instead of XDSUnknownPatientId) in pre-connectathon tests)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.11 NAME 'xdsAffinityDomainConfigDir'
  DESC 'Path to affinity domain configuration directory'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.20 NAME 'xdsRepositoryUID'
  DESC 'Unique ID of XDS.b Repository'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.21 NAME 'xdsRegistryURL'
  DESC 'Registry URL which is used by Repository to register document.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.22 NAME 'xdsLogFullMessageHosts'
  DESC 'Host name for which the full message (with documents) should be logged instead of SOAPpart'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.23 NAME 'xdsAllowedCipherHostname'
  DESC 'Hostname which is allowed in TLS hostname checking. Possible values: CERT , * (default) or <hostname>'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.24 NAME 'xdsForceMTOM'
  DESC 'Force MTOM/XOP message, even message doesn't contain attachment(s)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.30 NAME 'xdsHomeCommunityID'
  DESC 'XDS/XCA HomeCommunityID'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.31 NAME 'xdsRepositoryURL'
  DESC 'Repository URL(s) which is used by Source, Consumer and XCA Actors.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.32 NAME 'xdsRespondingGatewayURL'
  DESC 'Responding Gateway URL(s) which is used by XCA Initiating Gateway Actors.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.33 NAME 'xdsRespondingGatewayRetrieveURL'
  DESC 'Optional Responding Gateway Retrieve URL(s) if Query and Retrieve Service use different URLs.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.34 NAME 'xdsAsync'
  DESC 'Enable Async web service request'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.35 NAME 'xdsAsyncHandler'
  DESC 'Use async handler method for Async web service request'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.36 NAME 'xdsiSourceURL'
  DESC 'XDS-I Source URL(s) which is used by XDS-i Consumer and XCA-I Initiating Gateway actors.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.37 NAME 'xdsPIXConsumerApplication'
  DESC 'Application and Facility of PIX Consumer. Format: <application>^<facility>'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.38 NAME 'xdsPIXManagerApplication'
  DESC 'Application and Facility of PIX Manager. Format: <application>^<facility>'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.15.20.3.39 NAME 'xdsAssigningAuthority'
  DESC 'Assigning Authority'
  EQUALITY caseExactMatch
  SUBSTR caseExactSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
objectClasses: ( 1.2.40.0.13.1.15.20.4.1 NAME 'xdsRegistry'
  DESC 'XDS Registry information'
  SUP top STRUCTURAL
  MUST (
    cn $
    xdsApplicationName $
    xdsAffinityDomain $
    xdsAffinityDomainConfigDir )
  MAY (
    xdsAcceptedMimeTypes $
    xdsSoapMsgLogDir $
    xdsCreateMissingPIDs $
    xdsCreateMissingCodes $
    xdsDontSaveCodeClassifications $
    xdsCheckAffinityDomain $
    xdsCheckMimetype $
    xdsPreMetadataCheck ) )
objectClasses: ( 1.2.40.0.13.1.15.20.4.2 NAME 'xdsRepository'
  DESC 'XDS Repository information'
  SUP top STRUCTURAL
  MUST (
    cn $
    xdsApplicationName $
    xdsRegistryURL $
    xdsRepositoryUID )
  MAY (
    xdsAcceptedMimeTypes $
    xdsCheckMimetype $
    xdsSoapMsgLogDir $
    xdsAllowedCipherHostname $
    xdsForceMTOM $
    xdsLogFullMessageHosts ) )
objectClasses: ( 1.2.40.0.13.1.15.20.4.3 NAME 'xcaRespondingGW'
  DESC 'XCA Responding Gateway information'
  SUP top STRUCTURAL
  MUST (
    cn $
    xdsApplicationName $
    xdsHomeCommunityID $
    xdsRegistryURL $
    xdsRepositoryURL )
  MAY (
    xdsSoapMsgLogDir ) )
objectClasses: ( 1.2.40.0.13.1.15.20.4.4 NAME 'xcaInitiatingGW'
  DESC 'XCA Initiating Gateway information'
  SUP top STRUCTURAL
  MUST (
    cn $
    xdsApplicationName $
    xdsHomeCommunityID $
    xdsRespondingGatewayURL )
  MAY (
    xdsRespondingGatewayRetrieveURL $
    xdsRegistryURL $
    xdsRepositoryURL $
    xdsAsync $
    xdsAsyncHandler $
    xdsPIXConsumerApplication $
    xdsPIXManagerApplication $
    xdsAssigningAuthority $
    xdsSoapMsgLogDir ) )
objectClasses: ( 1.2.40.0.13.1.15.20.4.5 NAME 'xcaiRespondingGW'
  DESC 'XCA-I Responding Gateway information'
  SUP top STRUCTURAL
  MUST (
    cn $
    xdsApplicationName $
    xdsHomeCommunityID $
    xdsiSourceURL )
  MAY (
    xdsSoapMsgLogDir ) )
objectClasses: ( 1.2.40.0.13.1.15.20.4.6 NAME 'xcaiInitiatingGW'
  DESC 'XCA-I Initiating Gateway information'
  SUP top STRUCTURAL
  MUST (
    cn $
    xdsApplicationName $
    xdsHomeCommunityID $
    xdsRespondingGatewayURL )
  MAY (
    xdsiSourceURL $
    xdsAsync $
    xdsAsyncHandler $
    xdsSoapMsgLogDir ) )
